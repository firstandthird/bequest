{
  "name": "ajax",
  "version": "0.0.0",
  "description": "Ajax class",
  "main": "dist/ajax.js",
  "scripts": {
    "build": "mkdir -p dist && babel lib/ajax.js --out-file dist/ajax.js --source-maps",
    "dev": "concurrently -r 'nswatch' 'static-server -p 8000'",
    "lint": "eslint lib/ajax.js",
    "prepublish": "npm run build"
  },
  "watch": {
    "lib/ajax.js": ["lint", "build"]
  },
  "keywords": [
    "browser",
    "ajax",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/ajax.git"
  },
  "bugs": {
    "url": "https://github.com/firstandthird/ajax/issues"
  },
  "homepage": "https://github.com/firstandthird/ajax#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "concurrently": "^2.2.0",
    "eslint": "^3.5.0",
    "eslint-config-firstandthird": "^3.0.2",
    "eslint-plugin-import": "^1.15.0",
    "nswatch": "^0.2.0",
    "static-server": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "firstandthird"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
